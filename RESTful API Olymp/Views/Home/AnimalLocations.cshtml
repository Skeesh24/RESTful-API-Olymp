@using RESTful_API_Olymp.Domain.Entities
@model LocationEntity[]
@{
    Layout = "_Layout";
}

<h1>This is locations output</h1>
<h2>Please, enter yr query in the search line</h2>

@foreach (var location in Model)
{
    <div class="animalCard" style="background-color: aquamarine; margin: 20px; padding: 30px; border-radius:15px;">
        <p>
            ID: @location.Id <br />
            Date time of visiting location point: @location.DateTimeOfVisitLocationPoint <br />
            location point id: @location.locationPointId <br />
        </p>
    </div>
}

